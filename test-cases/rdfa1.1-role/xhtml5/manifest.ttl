@base <http://rdfa.info/test-suite/test-cases/rdfa1.1-role/xhtml5/manifest> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix mf: <http://www.w3.org/2001/sw/DataAccess/tests/test-manifest#> .
@prefix qt: <http://www.w3.org/2001/sw/DataAccess/tests/test-query#> .
@prefix test: <http://www.w3.org/2006/03/test-description#> .

<>  rdf:type mf:Manifest ;
    rdfs:comment "RDFa rdfa1.1-role tests for xhtml5" ;
    mf:entries (
      <#0305>
      <#0306>
      <#0307>
  ) .

<#0305> a mf:QueryEvaluationTest;
  mf:name """Test 0305: role attribute with explicit id and term""";
  rdfs:comment """When role is specified along with an id the appropriate triple is created.""";
  test:classification <http://www.w3.org/2006/03/test-description#required>;
  mf:action [ a qt:QueryTest;
    qt:queryForm qt:QueryAsk;
    qt:query <http://rdfa.info/test-suite/test-cases/rdfa1.1-role/xhtml5/0305.sparql>;
    qt:data <http://rdfa.info/test-suite/test-cases/rdfa1.1-role/xhtml5/0305.xhtml>
  ];
  test:specificationReference """
    http://www.w3.org/TR/role-attribute/#using-role-in-conjunction-with-rdfa
    says 'if @id is present, it is used to supply the subject...'
  """;
  mf:result true .

<#0306> a mf:QueryEvaluationTest;
  mf:name """Test 0306: role attribute with explicit base id and term""";
  rdfs:comment """When role is specified along with an id the appropriate triple is created.""";
  test:classification <http://www.w3.org/2006/03/test-description#required>;
  mf:action [ a qt:QueryTest;
    qt:queryForm qt:QueryAsk;
    qt:query <http://rdfa.info/test-suite/test-cases/rdfa1.1-role/xhtml5/0306.sparql>;
    qt:data <http://rdfa.info/test-suite/test-cases/rdfa1.1-role/xhtml5/0306.xhtml>
  ];
  test:specificationReference """
    http://www.w3.org/TR/role-attribute/#using-role-in-conjunction-with-rdfa
    says 'if @id is present, it is used to supply the subject...'
  """;
  mf:result true .

<#0307> a mf:QueryEvaluationTest;
  mf:name """Test 0307: role attribute with term and no id""";
  rdfs:comment """When role is specified along with an id the appropriate triple is created.""";
  test:classification <http://www.w3.org/2006/03/test-description#required>;
  mf:action [ a qt:QueryTest;
    qt:queryForm qt:QueryAsk;
    qt:query <http://rdfa.info/test-suite/test-cases/rdfa1.1-role/xhtml5/0307.sparql>;
    qt:data <http://rdfa.info/test-suite/test-cases/rdfa1.1-role/xhtml5/0307.xhtml>
  ];
  test:specificationReference """
    http://www.w3.org/TR/role-attribute/#using-role-in-conjunction-with-rdfa
    says 'if @id is present, it is used to supply the subject...'
  """;
  mf:result true .
